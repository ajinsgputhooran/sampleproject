#include <stdio.h>

int main() {
    // Declare variables
    int num1, num2, sum;

    // Prompt user for input
    printf("first number: ");
    scanf("%d", &num1);

    printf("second number: ");
    scanf("%d", &num2);

    // Calculate the sum
    sum = num1 + num2;

    // Display the result
    printf("The sum of %d and %d is: %d\n", num1, num2, sum);

    return 0;
}
